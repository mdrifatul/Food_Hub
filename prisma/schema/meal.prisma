model Meal {
  id String @id @default(uuid())
  title String? @db.VarChar(255)
  price Int
  authorId String
  cuisine String?
  dietaryPreferences String[]

  imageUrl String?
  isAvailable Boolean @default(true)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  orderItem OrderItem[]
  review Review[]
  

  @@index([authorId])
  @@map("meals")
}